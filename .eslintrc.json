{
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": "off",
    "padded-blocks": "off",
    "import/prefer-default-export": "off",
    "operator-linebreak": "off",
    "no-use-before-define": "off",
    "react/jsx-tag-spacing": "off",
    "react/no-array-index-key": "off",
    "react/prop-types": [2, {"ignore": ["children"]}]
  },
  "env": {
    "jest": true
  }
}